{
  "name": "ts-gantt",
  "version": "0.0.1",
  "description": "simple gantt chart written using typescript",
  "main": "dist/ts-gantt.js",
  "types": "dist/ts-gantt.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "buildts": "tsc && copyfiles -f ./src/*.css ./tsc/",
    "buildru": "rollup -c",
    "build": "tsc && copyfiles -f ./src/*.css ./tsc/ && rollup -c && dts-bundle-generator -o dist/tg-gantt.d.ts src/ts-gantt.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yermolim/ts-gantt.git"
  },
  "keywords": [
    "gantt",
    "chart",
    "typescript",
    "timeline"
  ],
  "author": "Volodymyr Yermolenko <yermolim@gmail.com> (https://github.com/yermolim)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yermolim/ts-gantt/issues"
  },
  "homepage": "https://github.com/yermolim/ts-gantt#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@rollup/plugin-strip": "^1.3.2",
    "@rollup/plugin-typescript": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "copyfiles": "^2.2.0",
    "dts-bundle-generator": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "rollup": "^2.1.0",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-terser": "^5.3.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
