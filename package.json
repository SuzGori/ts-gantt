{
  "name": "ts-gantt",
  "version": "0.0.1",
  "description": "simple gantt chart written using typescript",
  "main": "dist/ts-gantt.js",
  "types": "dist/ts-gantt.d.ts",
  "scripts": {
    "test": "karma start",
    "buildts": "tsc && copyfiles -f ./src/*.css ./tsc/",
    "buildru": "rollup -c",
    "build": "tsc && copyfiles -f ./src/*.css ./tsc/src && rollup -c && dts-bundle-generator -o dist/ts-gantt.d.ts src/ts-gantt.ts",
    "start": "npm run build && http-server ./ -a localhost -p 5500"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yermolim/ts-gantt.git"
  },
  "keywords": [
    "gantt",
    "chart",
    "typescript",
    "timeline"
  ],
  "author": "Volodymyr Yermolenko <yermolim@gmail.com> (https://github.com/yermolim)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yermolim/ts-gantt/issues"
  },
  "homepage": "https://github.com/yermolim/ts-gantt#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@rollup/plugin-strip": "^1.3.2",
    "@rollup/plugin-typescript": "^4.0.0",
    "@types/jasmine": "^3.5.10",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "copyfiles": "^2.2.0",
    "dts-bundle-generator": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "karma-typescript": "^5.0.1",
    "rollup": "^2.3.0",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-terser": "^5.3.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
