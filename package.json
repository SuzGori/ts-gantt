{
  "name": "ts-gantt",
  "version": "0.2.2",
  "description": "simple gantt chart written using typescript",
  "main": "dist/ts-gantt.umd.min.js",
  "module": "dist/ts-gantt.esm.min.js",
  "types": "dist/ts-gantt.d.ts",
  "scripts": {
    "test": "karma start",
    "buildts": "tsc && copyfiles -f ./src/*.css ./tsc/",
    "buildru": "rollup -c",
    "build": "tsc && copyfiles -f ./src/*.css ./tsc/src && rollup -c && dts-bundle-generator -o dist/ts-gantt.d.ts src/ts-gantt.ts",
    "start": "npm run build && copyfiles -f ./dist/styles.min.css ./dist/ts-gantt.umd.min.js ./demo && lite-server -c ls-config.json",
    "mpublish": "npm version minor && npm publish",
    "ppublish": "npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yermolim/ts-gantt.git"
  },
  "keywords": [
    "gantt",
    "svg",
    "grid",
    "table",
    "chart",
    "timeline",
    "typescript",
    "english",
    "ukrainian",
    "russian"
  ],
  "author": "Volodymyr Yermolenko <yermolim@gmail.com> (https://github.com/yermolim)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yermolim/ts-gantt/issues"
  },
  "homepage": "https://github.com/yermolim/ts-gantt#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@rollup/plugin-strip": "^1.3.2",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/jasmine": "^3.5.10",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "copyfiles": "^2.2.0",
    "dts-bundle-generator": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^5.0.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.0",
    "karma-jasmine": "^3.1.1",
    "karma-typescript": "^5.0.2",
    "lite-server": "^2.5.4",
    "puppeteer": "^3.0.2",
    "rollup": "^2.7.6",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-terser": "^5.3.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "dayjs": "^1.8.26"
  }
}
